{
  "name": "letletme_data",
  "version": "1.0.0",
  "description": "Letletme_data fetches data from the Fantasy Premier League servers, cleans and transforms the data, and then stores it in MongoDB and Redis, providing APIs for querying data.",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.js",
    "build": "npx prisma generate && tsc",
    "test": "jest --detectOpenHandles --forceExit",
    "coverage": "jest --coverage",
    "prisma": "npx prisma generate",
    "lint": "eslint . src --cache --fix",
    "format": "prettier --write . src",
    "test:db": "ts-node test-db.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonglam/letletme_data.git"
  },
  "keywords": [
    "FPL",
    "Letletme",
    "Node.js",
    "Express.js",
    "TypeScript"
  ],
  "author": "Tong Lam",
  "license": "MIT",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@types/node-fetch": "^2.6.11",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "prisma": "^6.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.4.0"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@supabase/supabase-js": "^2.46.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "redis": "^4.6.13",
    "zod": "^3.22.4"
  }
}

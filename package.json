{
  "name": "letletme_data",
  "version": "1.0.0",
  "description": "Letletme_data fetches data from the Fantasy Premier League servers, cleans and transforms the data, and then stores it in MongoDB and Redis, providing APIs for querying data.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.js",
    "build": "pnpm prisma generate && tsc",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prisma": "pnpm prisma generate",
    "lint": "eslint . --ext .ts,.tsx --cache --config eslint.config.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx --cache --fix --config eslint.config.mjs",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings=0 --config eslint.config.mjs",
    "lint:auto-fix": "eslint . --ext .ts,.tsx --fix --config eslint.config.mjs --rule '{\"import/order\": [\"error\", {\"groups\": [\"builtin\", \"external\", \"internal\", [\"parent\", \"sibling\"], \"index\"], \"newlines-between\": \"always\"}]}'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "check-all": "pnpm run lint && pnpm run format:check && pnpm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonglam/letletme_data.git"
  },
  "keywords": [
    "FPL",
    "Letletme",
    "Node.js",
    "Express.js",
    "TypeScript"
  ],
  "author": "Tong Lam",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitest/coverage-v8": "^3.1.1",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "nodemon": "^3.1.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.4",
    "@types/pino-http": "^5.8.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "bullmq": "^5.49.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.7.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "fp-ts": "^2.16.9",
    "helmet": "^8.1.0",
    "io-ts": "^2.2.22",
    "io-ts-types": "^0.5.19",
    "ioredis": "^5.6.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pino": "^9.6.0",
    "pino-http": "^10.3.0",
    "pino-roll": "^3.1.0",
    "postgres": "^3.4.5",
    "redis": "^4.6.11",
    "rotating-file-stream": "^3.2.5",
    "uuid": "^11.0.3",
    "zod": "^3.24.3"
  }
}

{
  "name": "letletme_data",
  "version": "1.0.0",
  "description": "Letletme_data fetches data from the Fantasy Premier League servers, cleans and transforms the data, and then stores it in MongoDB and Redis, providing APIs for querying data.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "nodemon src/index.js",
    "build": "npx prisma generate && tsc",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "prisma": "npx prisma generate",
    "lint": "eslint . --ext .ts,.tsx --cache --config eslint.config.mjs",
    "lint:fix": "eslint . --ext .ts,.tsx --cache --fix --config eslint.config.mjs",
    "lint:strict": "eslint . --ext .ts,.tsx --max-warnings=0 --config eslint.config.mjs",
    "lint:auto-fix": "eslint . --ext .ts,.tsx --fix --config eslint.config.mjs --rule '{\"import/order\": [\"error\", {\"groups\": [\"builtin\", \"external\", \"internal\", [\"parent\", \"sibling\"], \"index\"], \"newlines-between\": \"always\"}]}'",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "check-all": "npm run lint && npm run format:check && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tonglam/letletme_data.git"
  },
  "keywords": [
    "FPL",
    "Letletme",
    "Node.js",
    "Express.js",
    "TypeScript"
  ],
  "author": "Tong Lam",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitest/coverage-v8": "^1.6.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.2.5",
    "prisma": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "^1.6.1"
  },
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "@supabase/supabase-js": "^2.47.8",
    "@types/pino-http": "^5.8.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.2",
    "bullmq": "^5.41.5",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.5.0",
    "fp-ts": "^2.16.9",
    "helmet": "^8.0.0",
    "io-ts": "^2.2.22",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pino": "^9.6.0",
    "pino-http": "^10.3.0",
    "pino-roll": "^3.0.0",
    "redis": "^4.6.11",
    "rotating-file-stream": "^3.2.5",
    "uuid": "^11.0.3",
    "zod": "^3.22.4"
  }
}
